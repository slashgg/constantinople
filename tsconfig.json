{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": "src/app",
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "module": "commonjs",
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "importHelpers": true,
    "target": "es5",
    "typeRoots": ["node_modules/@types"],
    "paths": {
      "@env": ["./../environments/environment.ts"],
      "@utils": ["./../utils"],
      "@utils/*": ["./../utils/*"],
      "@root": ["./root"],
      "@root/*": ["./root/*"],
      "@core": ["./core"],
      "@core/*": ["./core/*"],
      "testlib": ["dist/testlib"],
      "testlib/*": ["dist/testlib/*"]
    },
    "lib": ["es2018", "dom"]
  }
}
